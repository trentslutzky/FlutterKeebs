outlines:
  exports:
    raw:
      - type: keys
        side: left
        size: [cx,cy]
    panel:
      - type: outline
        name: raw
        fillet: 0
    keys1:
      - type: keys
        side: left
        tags: [1u]
        size: [cx-0.5, cy-0.5]
        bound: false
    keys5:
      - type: keys
        side: left
        tags: [15u]
        size: [1.5cx-0.5, cy-0.5]
        bound: false
pcbs:
  esper:
    outlines:
      main:
        outline: panel
      caps:
        outline: keys1
        layer: Eco1.User
      caps15:
        outline: keys5
        layer: Eco1.User
    footprints:
      promicro:
        type: promicro_pretty
        anchor:
          ref: matrix_inner_home
          shift: [20, -7.25]
          rotate: -90
        params:
          orientation: down
      reset_button:
          type: b3u1000p
          nets:
            r1: RST
            r2: GND
          anchor:
            ref: matrix_inner_home
            shift: [31,2.5]
            rotate: -90
      power_switch:
          type: pcm12
          nets:
            from: pos
            to: RAW
          anchor:
            ref: ext_thumb_only_only
            shift: [13.5,0]
            rotate: 90
          params:
            reverse: true
      jst:
          type: bat
          nets: 
            pos: pos
            neg: GND
          anchor:
            ref: ext_thumb_only_only
            shift: [-7.0,11]
